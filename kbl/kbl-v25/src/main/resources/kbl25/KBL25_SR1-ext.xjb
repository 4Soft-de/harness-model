<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings xmlns:jxb="https://jakarta.ee/xml/ns/jaxb"
        xmlns:inheritance="http://jvnet.org/basicjaxb/xjc/inheritance"
        xmlns:ci="http://jaxb.dev.java.net/plugin/code-injector"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:nav="http://www.4soft.de/xjc-plugins/navigations"
        extensionBindingPrefixes="xjc dg"
        jxb:extensionBindingPrefixes="xjc annox xhtml"
        node="/xs:schema"
        schemaLocation="KBL25_SR1.xsd"
        version="3.0">

    <jxb:bindings multiple="true"
            node="//xs:complexType[xs:attribute[@name='id']]">
        <inheritance:implements>com.foursoft.harness.navext.runtime.model.Identifiable</inheritance:implements>
        <inheritance:implements>com.foursoft.harness.navext.runtime.model.ModifiableIdentifiable</inheritance:implements>
        <ci:code>
            @Override
            public String toString() {
            return this.getClass().getSimpleName() + "[" + this.getXmlId() + "]";
            }
        </ci:code>
    </jxb:bindings>

    <jxb:bindings node="//xs:attribute[@name='id']" multiple="true">
        <jxb:property name="xmlId"/>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Id']]">
        <inheritance:implements>com.foursoft.harness.kbl.common.HasIdentification</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Wire_number' ]]">
        <inheritance:implements>com.foursoft.harness.kbl.common.HasWireNumber</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true"
            node="//xs:complexType[@name='Accessory_occurrence'
                    or @name='Assembly_part_occurrence'
                    or @name='Cavity_plug_occurrence'
                    or @name='Cavity_seal_occurrence'
                    or @name='Co_pack_occurrence'
                    or @name='Component_box_occurrence'
                    or @name='Component_occurrence'
                    or @name='Connector_occurrence'
                    or @name='Fixing_occurrence'
                    or @name='General_wire_occurrence'
                    or @name='Special_terminal_occurrence'
                    or @name='Terminal_occurrence'
                    or @name='Wire_protection_occurrence']">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasPart</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true"
            node="//xs:complexType[@name='Accessory_occurrence'
                    or @name='Assembly_part_occurrence'
                    or @name='Cavity_plug_occurrence'
                    or @name='Cavity_seal_occurrence'
                    or @name='Co_pack_occurrence'
                    or @name='Component_box_occurrence'
                    or @name='Component_occurrence'
                    or @name='Connector_occurrence'
                    or @name='Fixing_occurrence'
                    or @name='General_wire_occurrence'
                    or @name='Special_terminal_occurrence'
                    or @name='Terminal_occurrence'
                    or @name='Wiring_group'
                    or @name='Accessory_occurrence'
                    or @name='Connection'
                    or @name='Wire_protection_occurrence']">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasParentAssemblyPart
        </inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[ @name='Assembly_part_occurrence']">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasSpecifiedOccurrences</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true"
            node="//xs:complexType[@name!='Alias_identification' and .//xs:element[@name='Alias_id']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasAliasIds</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Description']]">
        <inheritance:implements>com.foursoft.harness.kbl.common.HasDescription</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Installation_information']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasInstallationInformation</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Processing_information']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasProcessingInformation</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Reference_element']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasReferenceElement</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Description']]">
        <inheritance:implements>com.foursoft.harness.kbl.common.HasDescription</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Placement']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasPlacement</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Instruction_type']]">
        <inheritance:implements>com.foursoft.harness.kbl.common.HasInstruction</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Instruction_value']]">
        <inheritance:implements>com.foursoft.harness.kbl.common.HasInstruction</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[@name='KBL_container']">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasParts</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[@name='Harness']">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasLocatedComponents</inheritance:implements>
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasConnectionOrOccurrences</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Related_assembly']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasRelatedAssembly</inheritance:implements>
    </jxb:bindings>

    <jxb:bindings multiple="true" node="//xs:complexType[.//xs:element[@name='Related_occurrence']]">
        <inheritance:implements>com.foursoft.harness.kbl.v25.HasRelatedOccurrence</inheritance:implements>
    </jxb:bindings>

    <nav:selector name="LocatedComponent" implements="com.foursoft.harness.navext.runtime.model.Identifiable"/>
    <nav:selector name="LocatedComponent" implements="com.foursoft.harness.navext.runtime.model.ModifiableIdentifiable"/>
    <nav:selector name="LocatedComponent" implements="com.foursoft.harness.kbl.v25.PlacedElement"/>
    <nav:selector name="ConnectionOrOccurrence" implements="com.foursoft.harness.navext.runtime.model.Identifiable"/>
    <nav:selector name="ConnectionOrOccurrence" implements="com.foursoft.harness.navext.runtime.model.ModifiableIdentifiable"/>
    <nav:selector name="WireOrCoreOccurrence" implements="com.foursoft.harness.kbl.common.HasWireNumber"/>
    <nav:selector name="WireOrCoreOccurrence" implements="com.foursoft.harness.kbl.v25.HasInstallationInformation"/>
    <nav:selector name="PartUsageSelect" implements="com.foursoft.harness.kbl.common.HasIdentification"/>
    <nav:selector name="PartUsageSelect" implements="com.foursoft.harness.kbl.v25.HasInstallationInformation"/>
    <nav:selector name="PartUsageSelect" implements="com.foursoft.harness.kbl.v25.ConnectionOrOccurrence"/>

</jxb:bindings>
